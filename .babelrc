{
  "presets": ["es2015", "react"],
  "plugins": [
    "add-module-exports",
    "transform-object-rest-spread",
    "transform-class-properties",
    "syntax-decorators",
    "transform-react-constant-elements",
    "transform-react-inline-elements",
    ["react-intl", {
        "messagesDir": "./build/messages/"
        }],
  ]
}
